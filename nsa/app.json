{
  "expo": {
    "name": "nsa",
    "slug": "nsa",
    "version": "1.0.0",
    "orientation": "default",
    "userInterfaceStyle": "automatic",
    "scheme": "acme",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ]
    ],
    "splash": {
      "image": "./src/assets/images/Nerd_Stuff_Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1D434E"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/favicon.png"
      }
    },
    "assetBundlePatterns": ["**/*"]
  }
}
